    <script>


        $(window).bind('hashchange', function() {
            if (window.location.href == 'http://127.0.0.1:8000/contact') {
                $('.singe-contact').addClass('singe-active');
                $('.link-contact').addClass('link-active');
            }
            else {
                $('.singe-contact').removeClass('singe-active');
                $('.link-contact').removeClass('link-active');
            }
        });



        $(document).ready(function() {
            $('.js-scrollTo').on('click', function() { // Au clic sur un élément
                var page = $(this).attr('href'); // Page cible
                var speed = 750; // Durée de l'animation (en ms)
                $('html, body').animate( { scrollTop: $(page).offset().top }, speed ); // Go
                return false;
            });
        });




        $('.nav-burger').on('click', function() {
            $('.nav-mobile').addClass('burger-active');
        });
        $('.closeButton').on('click', function() {
            $('.nav-mobile').removeClass('burger-active');
        });

        var pageTracker = _gat._getTracker('UA-120903004-1');
        pageTracker._trackPageview();

    </script>
